<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>TestSelvet</servlet-name>
    <servlet-class>com.apache.web.TestSelvet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.apache.web.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MyForwardServlet</servlet-name>
    <servlet-class>com.apache.web.MyForwardServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MyForwardServlet2</servlet-name>
    <servlet-class>com.apache.web.MyForwardServlet2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadServlet</servlet-name>
    <servlet-class>com.DownAndUp.UploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownLoadServlet</servlet-name>
    <servlet-class>com.DownAndUp.DownLoadServlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>TestSelvet</servlet-name>
    <url-pattern>/TestSelvet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyForwardServlet</servlet-name>
    <url-pattern>/MyForwardServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyForwardServlet2</servlet-name>
    <url-pattern>/MyForwardServlet2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadServlet</servlet-name>
    <url-pattern>/UploadServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownLoadServlet</servlet-name>
    <url-pattern>/DownLoadServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
